{
  "name": "Fontier",
  "version" : "0.0",
  "minimum_chrome_version": "16.0.912",
  "manifest_version": 2,
  "description" : "The quickest way of changing the font size and default font size in Chrome.",
  "permissions": [
    "contentSettings",
    "experimental"
  ],
  "icons": {
    "128": "/img/icon128.png",
    "48": "/img/icon.png"
  },
  "browser_action": {
    "default_icon": "/img/icon.png",
    "default_popup": "/html/popup.html"
  },
   "content_scripts": [
    {
      "matches": ["http://jquery.com/*"],
      "js": ["/js/jquery-1.8.1.js"]
    }
  ]
}